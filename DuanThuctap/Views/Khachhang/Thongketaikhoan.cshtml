@model List<DuanThuctap.Models.StatisticalAccount>
@{

    ViewBag.Title = "Thống kê tài khoản";
    Layout = "~/Views/Shared/Header_Menu.cshtml";
}

<!--Side bar bên phải-->
<div class="col-9 p-2 shadow-sm bg-white border border-2 ml-1" style="border-radius: 10px;margin-top: 64px;">
    <main>
        <div class="container-fluid px-4">
            <h4 style="color:red;">Thống kê tài  khoản người dùng</h4>
            <hr />
            <h5 style="color:orange;">Số lượng theo loại tài khoản</h5>
            <div class="row">
                @foreach (var item in Model)
                {
                    <div class="col-4">
                        <div class="card bg-warning text-white mb-4">
                            <div class="card-body">@item.Categoryaccount</div>
                            <div class="card-footer d-flex align-items-center justify-content-between">
                                <a class="small text-white stretched-link" href="#">@item.AccountCount</a>
                                <div class="small text-white"><i class="fas fa-angle-right"></i></div>
                            </div>
                        </div>
                    </div>
                }
            </div>
            <!--Begin: Thống kê đăng ký-->
            
            <!--END: Thống kê đăng ký-->
        </div>
    </main>
</div>
<!--Side bar bên phải-->
